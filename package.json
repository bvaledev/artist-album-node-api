{
  "name": "artist-album-node-api",
  "version": "1.2.0",
  "description": "Api para cadastro e listagem de artistas e albuns musicais",
  "keywords": [
    "nodejs",
    "jest",
    "mongodb",
    "tdd",
    "solid",
    "album",
    "artistas"
  ],
  "author": "Brendo Vale",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/bvaledev/artist-album-node-api.git",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "up": "npm run build && docker-compose up",
    "up:detach": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.3"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.19",
    "@types/mongodb": "^3.6.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.17.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "faker": "^5.1.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.5.3",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
